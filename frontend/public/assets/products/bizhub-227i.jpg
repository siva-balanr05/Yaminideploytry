$(document).ready(function () {
    
        const Name_REGEXP = /[^a-zA-Z \s]+/g;
        const Email_REGEXP = /[^a-zA-Z 0-9 \.\@@\-\_]+/g;
        $("#username").on("keypress keyup", function (event) {;
            if (Name_REGEXP.test(event.key)) {
                event.preventDefault();
            }
            $(this).val($(this).val().replace(Name_REGEXP, ""));
        });
        $("#useremail").on("keypress keyup", function (event) {
            if(IsEmail($(this).val()))
            {
                $("#erEmail").html("");           
                
            }
            else
            {
                $("#erEmail").html("Please enter a valid email address.");
                
            }
            
            /*if (Email_REGEXP.test(event.key)) {
                event.preventDefault();
            }
            $(this).val($(this).val().replace(Email_REGEXP, ""));*/
        });
        
        $("#userphone").on("keypress keyup", function (event) {
            $(this).val($(this).val().replace(/[^\d].+/, ""));
            if ((event.which < 48 || event.which > 57)) {
                event.preventDefault();
            }
        });
         const Message_REGEXP = /[^a-zA-Z 0-9 \.\-\_\#\, \s]+/g;
        $("#messages").on("keypress keyup", function (event) {
            if (Message_REGEXP.test(event.key)) {
                event.preventDefault();
            }
            $(this).val($(this).val().replace(Message_REGEXP, ""));
        });
    });
</script>



 


